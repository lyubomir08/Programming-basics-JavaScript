function depositcalculator (input) {
let amountDeposited = Number(input[0]);
let termOfDeposit = Number(input[1]);
let annualInterestRate = Number(input[2]);
let accruedInterest = amountDeposited * annualInterestRate / 100;
let interestOneMonth = accruedInterest / 12;
let totalPrice = amountDeposited + termOfDeposit * interestOneMonth;
console.log(totalPrice);
}
depositcalculator(["200", "3", "5.7"]);
